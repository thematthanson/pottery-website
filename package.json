{
  "name": "pottery-website",
  "version": "1.0.0",
  "type": "module",
  "description": "A pottery collection website built with Vite, TailwindCSS, and DaisyUI.",
  "main": "server.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && touch dist/.nojekyll",
    "preview": "vite preview",
    "build:css": "tailwindcss -i ./src/css/styles.css -o ./dist/assets/styles.css",
    "watch:css": "tailwindcss -i ./src/css/styles.css -o ./dist/assets/styles.css --watch",
    "serve": "node server.js"
  },
  "keywords": [
    "pottery",
    "website",
    "vite",
    "tailwindcss",
    "daisyui"
  ],
  "author": "Your Name",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.3.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "vite": "^5.0.0",
    "vite-plugin-static-copy": "^2.2.0"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "daisyui": "^4.12.22",
    "emailjs-com": "^3.2.0"
  }
}
